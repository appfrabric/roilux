{
  "name": "tropical-wood",
  "version": "1.0.0",
  "description": "Tropical Wood - Premium wood products from Cameroon",
  "private": true,
  "scripts": {
    "dev": "docker-compose up",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm run serve",
    "install:all": "npm run install:frontend && npm run install:backend",
    "install:frontend": "cd frontend && npm ci",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "prestart": "npm run build"
  },
  "workspaces": [
    "frontend"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/appfrabric/roilux.git"
  },
  "keywords": [
    "wood",
    "timber",
    "cameroon",
    "plywood",
    "melamine",
    "veneer",
    "tropical-wood"
  ],
  "author": "Roilux",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}